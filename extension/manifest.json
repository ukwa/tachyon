{
   "manifest_version": 2,
   "name": "Tachyon - Browsing The Web Of The Past",
   "version": "1.0.0",
   "background": {
       "scripts": ["tachyon.js"]
   },
   "icons": { 
           "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" 
   },
   "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Tachyon - Browsing The Web Of The Past"
   },
   "permissions": [
       "webRequest",
       "<all_urls>",
       "webRequestBlocking",
       "tabs"
    ],
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
